{
    "name": "aiida-icl",
    "author": "Chris Sewell",
    "author_email": "chrisj_sewell@hotmail.com",
    "description": "AiiDA plugin interfacing with the Imperial College London cx1 HPC",
    "url": "https://github.com/aiidateam/aiida-scheduler-cx1",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Framework :: AiiDA"
    ],
    "version": "0.3.2",
    "entry_points": {
        "aiida.schedulers": [
            "pbspro_cx1 = aiida_icl.schedulers.pbspro_cx1:PbsproCx1Scheduler"
        ]
    },
    "include_package_data": true,
    "reentry_register": true,
    "install_requires": [
      "aiida-core==1.0.0b5",
      "six",
      "ruamel.yaml"
    ],
    "extras_require": {
      "testing": [
        "mock==2.0.0",
        "pgtest==1.2.0",
        "sqlalchemy-diff==0.1.3",
        "pytest==3.6.3",
        "wheel>=0.31",
        "coverage",
        "pytest-cov",
        "pytest-timeout",
        "pytest-regressions"
      ],
      "code_style": [
        "flake8<3.8.0,>=3.7.0",
        "yapf==0.26.0",
        "pre-commit==1.14.4"
      ]
    }}
